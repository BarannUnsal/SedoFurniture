@{
    ViewData["Title"] = "Gösterge Paneli";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="container-fluid p-0">
    <h1 class="h3 mb-3"><strong>Analitik</strong> Gösterge Paneli</h1>
    <div class="row">
        @await Component.InvokeAsync("ProductNumberDashboard")
        <div class="col-12 col-md-6 col-xxl-3 d-flex order-1 order-xxl-1">
            <div class="card flex-fill">
                <div class="card-header">
                    <h5 class="card-title mb-0">Takvim</h5>
                </div>
                <div class="card-body d-flex">
                    <div class="align-self-center w-100">
                        <div class="chart">
                            <div id="datetimepicker-dashboard"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        @await Component.InvokeAsync("Last10Product")
    </div>
</div>
